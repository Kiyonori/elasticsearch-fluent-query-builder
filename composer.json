{
    "name": "kiyonori/elasticsearch-fluent-query-builder",
    "description": "Elasticsearch Fluent Query Builder.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiyonori SAKAI ❄️",
            "email": "neco.am@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kiyonori\\ElasticsearchFluentQueryBuilder\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-date": "*",
        "elasticsearch/elasticsearch": "^8.17"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "laravel/pint": "^1.24"
    }
}
